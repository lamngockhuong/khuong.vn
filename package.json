{
  "name": "khuong.vn",
  "version": "1.0.0",
  "description": "Identity landing page for khương.vn",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:pages": "pnpm build && wrangler pages dev dist",
    "build": "pnpm sitemap && tsc && vite build",
    "sitemap": "npx tsx scripts/generate-sitemap.ts",
    "preview": "vite preview",
    "deploy": "pnpm build && wrangler pages deploy dist",
    "lint": "biome lint .",
    "format": "biome format . --write",
    "check": "biome check . --write",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [],
  "author": "Lâm Ngọc Khương",
  "license": "ISC",
  "packageManager": "pnpm@10.7.1",
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@cloudflare/workers-types": "4.20260103.0",
    "@types/node": "^25.0.3",
    "typescript": "5.9.3",
    "vite": "7.3.0",
    "wrangler": "4.54.0"
  }
}
